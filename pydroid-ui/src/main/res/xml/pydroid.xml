<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">

  <PreferenceCategory android:key="application_settings">

    <Preference
        android:key="@string/about_license_key"
        android:title="@string/about_license_title"/>

    <Preference
        android:key="@string/check_version_key"
        android:title="@string/check_version_title"/>

    <Preference
        android:key="@string/upgrade_info_key"
        android:summary="@string/upgrade_info_summary"
        android:title="@string/upgrade_info_title"/>

    <Preference
        android:key="@string/clear_all_key"
        android:summary="@string/clear_all_summary"
        android:title="@string/clear_all_title"/>

  </PreferenceCategory>

  <PreferenceCategory android:title="@string/support_pyamsoft">

    <Preference
        android:key="@string/rating_key"
        android:summary="@string/rating_summary"
        android:title="@string/rating_title"/>

  </PreferenceCategory>

</PreferenceScreen>