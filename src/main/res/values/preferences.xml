  <string name="about_license_key" tools:keep="@string/about_license_key">about_license</string>
  <string name="about_license_title" tools:keep="@string/about_license_title">Open Source Licenses</string>

  <string name="check_version_key" tools:keep="@string/check_version_key">check_version</string>
  <string name="check_version_title" tools:keep="@string/check_version_title">Check for Updates</string>

